{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": [
      "@types/react",
      "@types/react-native",
      "@types/three",
      "@react-three/fiber",
      "@react-three/fiber/jsx-runtime"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "react-jsx"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
